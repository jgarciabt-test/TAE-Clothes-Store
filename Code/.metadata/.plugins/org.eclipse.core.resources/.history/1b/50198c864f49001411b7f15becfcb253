public class ItemPageFragment  extends SherlockFragment{

	private Product product;
	
	public ItemPageFragment(Product product){
		this.product = product;
	}
	
	@Override
	public View onCreateView(LayoutInflater inflater, ViewGroup container,
			Bundle savedInstanceState) {
		
		ViewGroup rootView = (ViewGroup) inflater.inflate(R.layout.fragment_item_page, container);
	
		ImageView picture = (ImageView) rootView.findViewById(R.id.iv_item_detail_page);	
	
		return rootView;
	}

	@Override
	public void onSaveInstanceState(Bundle outState) {
		// TODO Auto-generated method stub
		super.onSaveInstanceState(outState);
	}

	
	
	
}
