package com.tae.store.model;

public class Category {

	String name;
	String url_pic;
	int lower_price;
	
	public Category(){
		
	}
	
	public Category(String name, String url_price, int lower_price){
		this.name = name;
		this.url_pic = url_price;
		this.lower_price = lower_price;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public String getUrl_pic() {
		return url_pic;
	}

	public void setUrl_pic(String url_pic) {
		this.url_pic = url_pic;
	}

	public int getLower_price() {
		return lower_price;
	}

	public void setLower_price(int lower_price) {
		this.lower_price = lower_price;
	}
	
	
}
